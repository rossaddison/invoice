        "yiisoft/db": "dev-master",
        "yiisoft/db-mysql": "dev-master",
        "yiisoft/definitions": "^3.4.0",
        "yiisoft/di": "^1.4",
        "yiisoft/error-handler": "^4.1",
        "yiisoft/factory": "^1.3",
        "yiisoft/files": "^2.0",
        "yiisoft/form": "^1.4",
        "yiisoft/form-model": "^1.0.1",
        "yiisoft/friendly-exception": "^1.1",
        "yiisoft/html": "^3.11",
        "yiisoft/http": "^1.2",
        "yiisoft/injector": "^1.2",
        "yiisoft/input-http": "^1.0.1",
